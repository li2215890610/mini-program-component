/* pages/plan/submit/submit.wxss */
@import '../../common/person/template.wxss';
@import '../../../font/stylesheet.wxss';

.plan-submit-container {
  padding: 20rpx 36rpx 232rpx 38rpx;
  box-sizing: border-box;
}

.plan-submit-container .plan-submit_title {
  font-size: 44rpx;
  font-family: PingFangSC-Semibold;
  font-weight: 600;
  color: rgba(0, 0, 0, 1);
  line-height: 60rpx;
  padding: 20rpx 0;
  border-bottom: solid 2rpx rgba(241, 240, 241, 1);
}

.submit_self_title {
  padding: 40rpx 0 12rpx;
  box-sizing: border-box;
}

.submit_who_title {
  padding: 110rpx 0 12rpx;
  box-sizing: border-box;
}

.submit_who_title>text:nth-of-type(1), .submit_self_title>text:nth-of-type(1) {
  display: inline-block;
  width: 60rpx;
  height: 60rpx;
  background: rgba(41, 60, 122, 1);
  border-radius: 50%;
  font-size: 36rpx;
  font-family: PingFangSC-Semibold;
  font-weight: 600;
  color: rgba(255, 255, 255, 1);
  line-height: 60rpx;
}

.submit_who_title .submit_who_title_right>view:nth-of-type(1),
.submit_self_title>text:nth-of-type(2) {
  display: inline-block;
  font-size: 32rpx;
  font-family: PingFangSC-Semibold;
  font-weight: 600;
  color: rgba(0, 0, 0, 1);
  line-height: 44rpx;
  margin-left: 20rpx;
}

.submit_who_title .submit_who_title_right>view:nth-of-type(2) {
  font-size: 20rpx;
  font-family: PingFangSC-Regular;
  font-weight: 400;
  color: rgba(0, 0, 0, 1);
  line-height: 28rpx;
  margin-left: 20rpx;
}

.submit_who_btns {
  margin: 40rpx 0 30rpx;
}

.submit_who_btns>view {
  padding: 20rpx 52rpx;
  font-size: 28rpx;
  font-family: PingFangSC-Semibold;
  font-weight: 600;
  color: rgba(41, 60, 122, 1);
  line-height: 40rpx;
  border: solid 2rpx rgba(41, 60, 122, 1);
}

.submit_who_btns>view.active {
  color: rgba(255, 255, 255, 1);
  border-color: rgba(41, 60, 122, 1);
  background: rgba(41, 60, 122, 1);
  /* background: url(data:image/png;base64,) no-repeat;
  background-size: 48rpx 48rpx;
  background-position: top right; */
}

.submit_who_btns>view:nth-of-type(1) {
  margin-right: 20rpx;
}

.contact-enter {
  right: -34rpx;
  top: 0;
  padding: 42rpx 38rpx 38rpx;
  box-sizing: border-box;
  z-index: 3;
}

.submit_who_item .icon-name {
  display: inline-block;
  background: url(data:image/png;base64,) no-repeat;
  background-size: 100% 100%;
  width: 40rpx;
  height: 40rpx;
}

.submit_who_item.submit_who_nav {
  margin-top: 60rpx;
  border-bottom: 2rpx solid rgba(151, 151, 151, 1);
  padding: 20rpx 0;
  font-size: 28rpx;
  font-family: PingFangSC-Semibold;
  font-weight: 600;
  color: rgba(0, 0, 0, 1);
  line-height: 40rpx;
}

.submit_who_item.submit_who_nav>view:nth-of-type(2) {
  background: url(data:image/png;base64,) no-repeat;
  width: 37rpx;
  height: 40rpx;
  background-size: 100% 100%;
}

.submit_who_add {
  padding: 20rpx 0;
  font-size: 28rpx;
  font-family: PingFangSC-Semibold;
  font-weight: 600;
  color: rgba(0, 0, 0, 1);
  line-height: 40rpx;
  margin: 40rpx 0 32rpx;
  border: 2rpx solid rgba(151, 151, 151, 1);
}

.submit_who_desc {
  font-size: 28rpx;
  font-family: PingFangSC-Regular;
  font-weight: 400;
  color: rgba(0, 0, 0, 1);
  line-height: 40rpx;
}

.submit_who_desc>navigator {
  color: rgba(74, 144, 226, 1);
}

.plan-submit {
  width: 100%;
  height: 120rpx;
  bottom: 0;
  left: 0;
  background: rgba(255, 255, 255, 1);
  z-index: 5;
  border-top: solid 2rpx #f5f5f5;
}

.plan-submit>view, .plan-submit>form {
  line-height: 120rpx;
}

.plan-submit>view:nth-of-type(1) {
  font-size: 48rpx;
  font-family: Agency FB;
  color: rgba(30, 30, 30, 1);
  padding: 0 64rpx 0 68rpx;
}

.plan-submit>view:nth-of-type(1)>text:nth-of-type(2) {
  font-size: 20rpx;
  font-family: PingFangSC-Semibold;
  font-weight: 600;
  color: rgba(0, 0, 0, 1);
}

.plan-submit>form {
  background: rgba(34, 34, 33, 1);
  font-size: 36rpx;
  font-family: PingFangSC-Regular;
  font-weight: 400;
  color: rgba(255, 255, 255, 1);
  letter-spacing: 2rpx;
  flex-grow: 1;
}

button {
  padding: 0;
  line-height: 120rpx;
  height: 100%;
  width: 100%;
  border-radius: 0;
  -webkit-tap-highlight-color: rgba(0, 0, 0, 0);
  background: rgba(41, 60, 122, 1);
  color: rgba(255, 255, 255, 1);
}

.button-hover {
  background-color: rgba(41, 60, 122, 1);
  color: rgba(255, 255, 255, 1);
}

button::after {
  border: none;
}

.submit_who_self .submit_who_email, .submit_self_info .submit_who_type,
.submit_who_self .submit_who_tel {
  display: none;
}

.submit_who_btns~.submit_who_list>view:nth-of-type(1) .submit_who_nav:nth-of-type(1)>view:nth-of-type(2) {
  display: none;
}

.submit_who_self~.submit_who_list>view:nth-of-type(1) .submit_who_nav:nth-of-type(1)>view:nth-of-type(2) {
  display: block;
}

.picker-mask {
  position: fixed;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
  z-index: 10;
  background: rgba(0, 0, 0, 0.3);
  display: none;
}

.picker-container {
  position: fixed;
  left: 0;
  bottom: 0;
  width: 750rpx;
  background: white;
  transform: translateY(150%);
  transition: all 0.4s ease;
  z-index: 11;
}

.picker-show .picker-container {
  transform: translateY(0);
}

.picker-show .picker-mask {
  display: block;
}

.picker-title {
  padding: 30rpx 36rpx;
  box-sizing: border-box;
  border-bottom: solid 2rpx rgba(213, 212, 213, 1);
}

.picker-title>view {
  font-size: 28rpx;
  font-family: PingFangSC-Medium;
  font-weight: 500;
  color: rgba(74, 74, 74, 1);
  line-height: 40rpx;
  letter-spacing: 2rpx;
}

.picker-title>text {
  font-size: 60rpx;
  transform: rotate(45deg);
  color: rgba(216, 215, 216, 1);
  line-height: 40rpx;
  top: -4rpx;
  right: -6rpx;
}

.picker-content {
  max-height: 800rpx;
}

.picker-content .content-item {
  padding: 20rpx 36rpx;
  box-sizing: border-box;
}

.picker-content .avatar {
  width: 100rpx;
  height: 100rpx;
  border-radius: 50%;
  margin-right: 40rpx;
}

.picker-content .name {
  font-size: 28rpx;
  font-family: PingFangSC-Medium;
  font-weight: 500;
  color: rgba(74, 74, 74, 1);
  line-height: 40rpx;
  letter-spacing: 2rpx;
}

.picker-content .passport {
  font-size: 24rpx;
  font-family: PingFangSC-Regular;
  font-weight: 400;
  color: rgba(74, 74, 74, 1);
  line-height: 34rpx;
  letter-spacing: 2rpx;
}
